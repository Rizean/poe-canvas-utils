{
  "name": "@your-npm-username/poe-canvas-utils",
  "version": "0.1.0",
  "description": "Utility hooks for Poe Canvas integration and logging.",
  "type": "module",
  "main": "./dist/poe-canvas-utils.umd.cjs",
  "module": "./dist/poe-canvas-utils.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/poe-canvas-utils.js",
      "require": "./dist/poe-canvas-utils.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "prepublishOnly": "pnpm build",
    "test": "jest --watchAll --passWithNoTests",
    "--AI-Prompts--": "echo --AI-Prompts--",
    "ph-src": "ph --code src --ignore \"**/*.test.ts\"",
    "ph-src-test": "ph --code src",
    "ph-dep": "ph --dependency-graph",
    "ph-complexity": "ph --complexity"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3 || ^19.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "prompt-helper": "^2.1.0",
    "react": "^19.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1"
  },
  "keywords": [
    "react",
    "hooks",
    "poe-api",
    "poe-canvas",
    "logger",
    "utils"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/poe-canvas-utils.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/poe-canvas-utils/issues"
  },
  "homepage": "https://github.com/your-username/poe-canvas-utils#readme"
